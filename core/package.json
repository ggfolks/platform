{
  "name": "tfw.core",
  "version": "1.0.0",
  "description": "Core abstractions for tfw platform",
  "author": "tfw, inc. (https://tfw.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfwdev/platform.git"
  },
  "sideEffects": false,
  "scripts": {
    "test": "jest --verbose",
    "docs": "typedoc src"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test.*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "awesome-typescript-loader": "^5.2.0",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.4"
  }
}
