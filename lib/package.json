{
  "name": "@tfwdev/platform",
  "version": "0.2.0",
  "description": "The tfw multiplayer game platform",
  "author": "tfw, inc. (https://tfw.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfwdev/platform.git"
  },
  "publishConfig": {
    "registry": "https://build1.tfw.dev/registry/"
  },
  "sideEffects": false,
  "files": [
    "**/*.js",
    "**/*.d.ts",
    "**/*.d.ts.map"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "jest --verbose",
    "wtest": "jest --verbose --watch",
    "docs": "typedoc ."
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test.*.(ts|js)"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "dependencies": {
    "@types/cannon": "^0.1.4",
    "@types/gl-matrix": "^2.4.5",
    "cannon": "^0.6.2",
    "firebase": "^7.24.0",
    "firebase-admin": "^8.8.0",
    "gl-matrix": "^3.0.0",
    "three": "^0.141.0",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/three": "^0.141.0",
    "@types/ws": "^6.0.2",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.1.0",
    "lodash": "^4.17.13",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.14.2",
    "typescript": "^4.5.4"
  }
}
