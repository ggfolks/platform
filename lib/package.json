{
  "name": "tfw",
  "version": "1.0.0",
  "description": "The tfw multiplayer game platform",
  "author": "tfw, inc. (https://tfw.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfwdev/platform.git"
  },
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "jest --verbose",
    "wtest": "jest --verbose --watch",
    "docs": "typedoc src"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/test.*.(ts|js)"
    ]
  },
  "dependencies": {
    "@types/cannon": "^0.1.4",
    "@types/gl-matrix": "^2.4.5",
    "cannon": "^0.6.2",
    "gl-matrix": "^3.0.0",
    "three": "^0.106.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "awesome-typescript-loader": "^5.2.0",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1"
  }
}
